rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are for development only and are not secure.
    // They allow any authenticated user to read and write all data.
    // Replace these with more secure rules before launching to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
